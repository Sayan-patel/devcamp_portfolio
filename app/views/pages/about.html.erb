<h1> Team-List</h1>
<hr>
<div class="row">
	<div class="col-md-6">
		<p>Mumbai Indians</p>
		<p>Chennai Super Kings</p>
		<p>Sunrisers Hyderabad</p>
		<p>Kolkata Knight Riders</p>
	</div>
	<div class="col-md-6">
		<p>Kings XI Punjab</p>
		<p>Rajasthan Royals</p>
		<p>Delhi Capitals</p>
		<p>Royal Challengers Bangalore</p>
	</div>
</div>
<div class="col-md-12">
	<p>
		<button 
			class = "btn btn-primary"
			type = "button"
			data-toggle = "collapse"
			data-target = "#cupWinners"
			aria-expanded = "false"
			aria-controller ="cupWinners"
		>
			Cup Winners
		</button>
	</p>

	<p>
		<div class ="collapse" id="cupWinners">
			<div class = "card card-block cupwinner-csk">
				Chennai Super Kings
			</div>
			<div class = "card card-block cupwinner-mi">
				Mumbai Indians
			</div>
		</div>
	</p>
</div>
<div class="col-md-12">
	<%= image_tag "ipl_cover.jpeg",width: 1000, alt: "Profile Image" %>
	<div class="card blacky">
		<div class="skills">
			<% @skills.each do |skill| %>
				<h4><%= skill.title %></h4>
				<div class="progress">
					<div class= "progress-bar progress-bar-stripped bg-warning" role ="progressbar" aria-valuenow = "<%= skill.percent_utilized %>" aria-valuemax = "100" style="width: <%= skill.percent_utilized %>%" >
					</div>
				</div>
			<% end %>
		</div>
	</div>
</div>

